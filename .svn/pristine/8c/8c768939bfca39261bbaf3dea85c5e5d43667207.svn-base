<ion-header>
  <ion-navbar color="primary">
    <button ion-button menuToggle>
      <ion-icon name="menu"></ion-icon>
    </button>
    <ion-title>Dropped Passengers</ion-title>
  </ion-navbar>
</ion-header>
<ion-content padding>
    
        <ion-list>
            <ion-card>
            <ion-item>
                <ion-label><strong>PNR</strong></ion-label>
                <ion-label><strong>WL NO</strong></ion-label>

                <ion-label><strong>NAME</strong></ion-label>
                <ion-label><strong>S/A</strong></ion-label>

                <ion-label><strong>SRC</strong></ion-label>
                <ion-label><strong>DEST</strong></ion-label>

            </ion-item>
                </ion-card>  
            <ion-card>

            <ion-item  *ngFor="let item of droppedPsgnArr">
                <ion-label>{{ item.PNR_NO }}</ion-label>
                <ion-label>{{ item.WAITLIST_NO }}</ion-label>

                <ion-label>{{ item.PSGN_NAME }}</ion-label>
                <ion-label>{{ item.AGE_SEX }}</ion-label>

                <ion-label>{{ item.BOARDING_POINT }}</ion-label>
                <ion-label>{{ item.RES_UPTO }}</ion-label>

            </ion-item>
                </ion-card>  

        </ion-list>
     <button ion-button (click)="backToMain()" round>Back</button>

</ion-content>
    

