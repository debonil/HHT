<!--
  Generated template for the ChartTabsPage page.

  See http://ionicframework.com/docs/components/#navigation for more info on
  Ionic pages and navigation.
-->
<ion-header>
    <ion-navbar>
        <ion-row>
            <ion-col> 
                <button ion-button menuToggle>
                    <ion-icon style ="color : white; font-size: 26px;" name="menu"></ion-icon>
                </button>
            </ion-col>
                  <ion-title>CHART PAGE</ion-title>

            <ion-col>
                <button ion-button full style = "background-color: transparent; padding:0; margin:0;">
                    <ion-icon style ="color : white; font-size: 26px;" name="loop"></ion-icon>
                </button>
                <p class="get-data"></p>
            </ion-col>
                <button ion-button class = "syncbtn" [class.dirtyClass]="isSecondary" (click) = "syncData()">
            <ion-icon name="sync"  style =" font-size: 20px;"></ion-icon>
        </button>
            
        </ion-row>
    </ion-navbar>
</ion-header>


<ion-content padding>
  <super-tabs id="mainTabs" (tabSelect)="onTabSelect($event)"selectedTabIndex="0">
           <super-tab  *ngFor="let tab of chartTab" [root]="tab.root" title="{{tab.title}}" [rootParams]= 'tabparams' id={{tab.title}} ></super-tab>

  </super-tabs>
</ion-content>
