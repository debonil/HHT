.alignleft {
	float: left;
	margin-left: 40px;
}
.alignright {
	float: right;
	margin-right: 40px;
}
.toggle-md.toggle-disabled, .item-md.item-toggle-disabled ion-label {
	opacity: 1;
    pointer-events: none;
}
a[disabled], button[disabled], [ion-button][disabled] {
    cursor: default;
    opacity: .9;
    pointer-events: none;
}
.status{
	transition: transform 1s;
	transform: rotate(1080deg);
}