page-home {

#custom-overlay {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 100000;
  width:100%;
  background-color: #002a3e;
}

.flb {
  background-color: white;
  height:100%;
  width:100%;
  animation: pulse 1s linear forwards;
  display: flex;
  align-items: center;
  justify-content: center;
}
.Aligner-item {
  max-width: 50%;
}
.Aligner-item--top {
  align-self: flex-start;
}
.Aligner-item--bottom {
  align-self: flex-end;
}
#custom-overlay img {
  display: block;
  margin: 0 auto;
  width: 50%;
  height: auto;
  animation: animation 3100ms linear infinite both;
  animation-delay: 1s;
}

@keyframes pulse {
  0% {
    opacity:0;
  }
  100% {
    opacity:1;
  }
}

ion-scroll {
    position: inherit;
    display: block;
}
.cris-title {
  font-size: large;
  .scroll-content{
     overflow: hidden;

}

 .toolbar-title{

    font-family: 'roboto-medium';
    font-size:30px; 
    color:#818181;
   } 


}
   

  .label-danger {
            background-color: transparent;
            color : rgb(165,42,42);
            font-size: 14px;
            font-style: italic;
    }

.item.item-input {
    background-color: transparent;
    font-weight: normal;
    -moz-border-radius: 40px;
    border-radius: 40px;
    border:solid 1px #8fa8e8;
    padding: 5px 5px 5px 25px;
    font-family: 'ubuntu-light';
    font-size:18px;
    color: aliceblue;


      .text-input:-moz-placeholder { /* Firefox 18- */
        color: 	#7a9cf1 !important;  
        }

        .text-input::-moz-placeholder {  /* Firefox 19+ */
        color: 	#7a9cf1 !important;  
        }

        .text-input:-ms-input-placeholder {  
        color: #7a9cf1 !important;  
        }

         .text-input::-webkit-input-placeholder {
        color:	#7a9cf1 !important;
        }

}

.user-avatar {
       -webkit-border-radius: 50%;
               border-radius: 50%;
               width :83px;
   }

.cris-input {
background-color: transparent;
}


.item >.item-inner{
   border-bottom-color: transparent !important;
   height : 50%;
 }

.button-round-md {
   padding: 2.7rem;
   border: solid 3px #8fa8e8;}

}

.text-input-md {
    margin: 0px 8px 0px 8px;
}


.item-md ion-icon[item-left] {
    margin-top: 0;
    margin-bottom: 0;

}

.item-block{
    min-height:36px;
}


.item-md.item-input.ng-invalid.ng-touched:not(.input-has-focus) .item-inner {
    border-bottom-color: transparent !important;
    box-shadow: none !important;
}

.list-md .item-input:last-child{
    border-bottom:solid 1px #8fa8e8;
}


.label-md {
    margin: 0 8px 0 0;
}

.item-md.item-input.input-has-focus .item-inner,
.list-md .item-input.ng-invalid.ng-touched:not(.input-has-focus):last-child,
.list-md .item-input.input-has-focus:last-child{
    box-shadow:none !important;
   
}

.list-md .item-input.ng-invalid.ng-touched:not(.input-has-focus):last-child {
   border:solid 1px #8fa8e8;
   
}

.list-md .item-input.input-has-focus:last-child{
    border-bottom:solid 1px #8fa8e8 !important;
}

.item-md.item-input.ng-valid.input-has-value:not(.input-has-focus) .item-inner {
    border-bottom-color: none;
    box-shadow: none;
}

.showpassword{
position: absolute;
top: 0;
right: 0;
color : #818181;

}

.showpassword.button-md {
    background-color: transparent !important;
}

.item.item-md .checkbox-md {
    position: static;
    display: block;
    margin: 9px 21px 9px 9px;
}

.showPasswordLabel.label.label-md {
  font-family: 'ubuntu-light';
  color : #333;
}


/* for ios only */



ion-header .toolbar-background.toolbar-background-ios {
    border-color: #3a65d2;
    background: #3a65d2;
}

ion-navbar.toolbar.toolbar-ios.statusbar-padding {
    min-height: 10px !important;
    max-height: 50px !important;
    padding: 0 !important;
}

ion-navbar.toolbar.toolbar-ios.toolbar-ios-transparent.statusbar-padding {
    max-height: 100% !important;
}

ion-title.cris-title.title.title-ios {
    display: block;
    position: relative;
}

.text-input-ios,
.label-ios {
    margin: 0px 8px 0px 8px;
}

ion-list .list-ios > .item-block:last-child, .list-ios > .item-wrapper:last-child .item-block {
    border-bottom: none !important;
}

ion-list ion-label#lbl-16 {
    color: #333;
    font-size: 16px;
}

ion-list.cris-input.list.list-ios {
    margin-top: 5px;
}

/* end for ios only */

